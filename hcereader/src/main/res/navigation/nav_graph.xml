<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainFragment">

    <fragment
        android:id="@+id/MainFragment"
        android:name="com.app.hcereader.MainFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_main">

        <action
            android:id="@+id/action_MainFragment_to_NfcFragment"
            app:destination="@id/NfcFragment" />
    </fragment>

    <fragment
        android:id="@+id/NfcFragment"
        android:name="com.app.hcereader.nfc.NfcFragment"
        android:label="@string/nfc_fragment_label"
        tools:layout="@layout/fragment_nfc" />
</navigation>